{
  "name": "@ncalteen/fast-track",
  "description": "FastTrack (React/Next.js)",
  "author": "Nick Alteen <ncalteen@github.com>",
  "version": "1.0.0",
  "private": true,
  "main": "src/bin/www",
  "type": "module",
  "homepage": "https://github.com/ncalteen/fast-track",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncalteen/fast-track.git"
  },
  "bugs": {
    "url": "https://github.com/ncalteen/fast-track/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "next:build": "next build",
    "next:dev": "next dev",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "prisma:format": "npx dotenv-cli -e .env.local -- npx prisma format",
    "prisma:generate": "npx dotenv-cli -e .env.local -- npx prisma generate",
    "prisma:migrate": "npx dotenv-cli -e .env.local -- npx prisma migrate dev",
    "prisma:push": "npx dotenv-cli -e .env.local -- npx prisma db push",
    "prisma:studio": "npx dotenv-cli -e .env.local -- npx prisma studio",
    "lint": "npx eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run prettier:write && npm run lint && npm run test && npm run coverage && npm run build"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml"
  },
  "dependencies": {
    "@azure/identity": "^4.6.0",
    "@hookform/resolvers": "^3.9.1",
